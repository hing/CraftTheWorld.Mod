                            <time value="3" />
                        </behaviour>
                    </gameobject>
                </event>
                <event value="quake">
                    <behaviour value="EarthQuakeBehaviour">
                        <freq value="5" />
                        <amp value="2" />
                        <duration value="0.3" />
                    </behaviour>
