  <!--влажность-->
  <humidity>
    <low local="HumLow">
      <rainInterval value="100-500"/>      <!--интервал между дождями-->
      <rainDuration value="10-30"/>      <!--продолжительность дождя-->
      <mrain value="65%"/>      <!--вероятность грибного дождя-->
      <rain value="30%"/>      <!--вероятность обычного дождя-->
      <storm value="5%"/>      <!--вероятность шторма-->
      <waterAbsorb value="200"/>      <!--базовое время впитывания воды в формуле-->
      <smallWARatio value="0.2"/>      <!--коэф. для базового времени впитывания воды в формуле для одной полоски воды-->
      <waterPondsArea value="5%"/>      <!--процент длины водоемов от длины уровня-->
      <waterPondHeight value="2-4"/>      <!--максимальная глубина -->
      <waterPondWidth value="5-10"/>      <!--максимальная ширина -->
    </low>
    <normal local="HumNormal">
      <rainInterval value="60-300"/>      <!--интервал между дождями-->
      <rainDuration value="20-50"/>      <!--продолжительность дождя-->
      <mrain value="60%"/>      <!--вероятность грибного дождя-->
      <rain value="30%"/>      <!--вероятность обычного дождя-->
      <storm value="10%"/>      <!--вероятность шторма-->
      <waterAbsorb value="300"/>      <!--базовое время впитывания воды в формуле-->
      <smallWARatio value="0.33"/>      <!--коэф. для базового времени впитывания воды в формуле для одной полоски воды-->
      <waterPondsArea value="13%"/>      <!--процент длины водоемов от длины уровня-->
      <waterPondHeight value="2-5"/>      <!--максимальная глубина -->
      <waterPondWidth value="5-10"/>      <!--максимальная ширина -->
    </normal>
    <high local="HumHigh">
      <rainInterval value="40-200"/>      <!--интервал между дождями-->
      <rainDuration value="25-60"/>      <!--продолжительность дождя-->
      <mrain value="40%"/>      <!--вероятность грибного дождя-->
      <rain value="30%"/>      <!--вероятность обычного дождя-->
      <storm value="30%"/>      <!--вероятность шторма-->
      <waterAbsorb value="350"/>      <!--базовое время впитывания воды в формуле-->
      <smallWARatio value="0.33"/>      <!--коэф. для базового времени впитывания воды в формуле для одной полоски воды-->
      <waterPondsArea value="25%"/>      <!--процент длины водоемов от длины уровня-->
      <waterPondHeight value="2-5"/>      <!--максимальная глубина -->
      <waterPondWidth value="5-10"/>      <!--максимальная ширина -->
    </high>
  </humidity>

